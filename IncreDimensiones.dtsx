              name="Leer Productos Data Stage"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        Categoria.idCategoria, Categoria.NombreCategoria, Familia.idFamilia, Familia.NombreFamilia, Producto.idProducto, Producto.NombreProducto, Rubro.idRubro, Rubro.NombreRubro, Seccion.idSeccion, Seccion.NombreSeccion, 
                         SubFamilia.idSubFamilia, SubFamilia.NombreSubFamilia
FROM            Categoria INNER JOIN
                         Familia ON Categoria.idCategoria = Familia.idCategoria INNER JOIN
                         Rubro ON Categoria.idRubro = Rubro.idRubro INNER JOIN
                         Seccion ON Rubro.idSeccion = Seccion.idSeccion INNER JOIN
                         SubFamilia ON Familia.idFamilia = SubFamilia.idFamilia INNER JOIN
                         Producto ON SubFamilia.idSubFamilia = Producto.idSubFamilia</property>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">30</property>
